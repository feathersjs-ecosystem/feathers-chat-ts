{
  "name": "feathers-chat",
  "description": "A FeathersJS real-time chat application written in TypeScript",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "David Luecke",
    "email": "daff@neyeon.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^11.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run compile && npm run coverage",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "clean": "shx rm -rf test/data/",
    "coverage": "nyc npm run mocha",
    "mocha": "npm run clean && NODE_ENV=test mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc",
    "update-dependencies": "ncu -u"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.15",
    "@feathersjs/authentication-local": "^4.5.15",
    "@feathersjs/authentication-oauth": "^4.5.15",
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/errors": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@feathersjs/socketio": "^4.5.15",
    "@seald-io/nedb": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-nedb": "^6.0.0",
    "helmet": "^5.1.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/serve-favicon": "^2.5.3",
    "axios": "^0.27.2",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "npm-check-updates": "^13.0.3",
    "nyc": "^15.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.2"
  }
}
